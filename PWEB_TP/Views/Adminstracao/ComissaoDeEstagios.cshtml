@model PWEB_TP.Models.ComissaoDeEstagiosViewModel

@{
    ViewBag.Title = "Comissão de Estágios";
}

<h3>Comissão De Estagios</h3>

<table class="table">
    <tr>
        <th>
            @Html.Label("Nome Completo")
        </th>
        <th>
            @Html.Label("Email")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Comissao) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Remover", "ComissaoDeEstagiosRemove", new { id = item.Id })
        </td>
    </tr>
}
</table>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>

        @Html.DropDownList("Escolhido", (SelectList)ViewData["NaoComissao"], "-- Select Category --", new { @class = "form-control" })


        <input type="submit" value="Adicionar Docente" class="btn btn-default" />
    </div>

    <div class="form-group">
        <div class="col-md-10">
        </div>
    </div>


}